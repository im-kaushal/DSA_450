//Linear search approach

import java.io.*;

class Solution {
	public static void main (String[] args) {
		System.out.println("GfG!");
		int[] arr = {3,8,19,2,16,5,1};
		int min=arr[0], max=0;
		for(int i=0; i<arr.length; i++)
		{
		    if(arr[i]<min){
		        min = arr[i];
		    }
		    if(arr[i]>max){
		        max = arr[i];
		    }
		}
		System.out.println(min);
		System.out.println(max);
	}
}
//Time complexity = O(n)

